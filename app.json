{
  "expo": {
    "name": "ARMi",
    "slug": "armi",
    "version": "1.3.13",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "scheme": "armi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.armi.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "ARMi needs access to your photo library to let you pick a profile picture.",
        "NSCameraUsageDescription": "ARMi needs camera access so you can take a new profile picture.",
        "NSPhotoLibraryAddUsageDescription": "ARMi may save photos to your library if you choose to keep them."
      },
      "buildNumber": "54"
    },
    "android": {
      "package": "com.armi.app",
      "versionCode": 54,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "05e77375-19f3-4420-aaf1-28b95256972d"
      },
